{
    "extends": "airbnb",
    "rules": {
        "no-undef": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "import/no-unresolved": 0,
        "react/prop-types": 0,
        "no-console": 0,
        "react/jsx-props-no-spreading": 0,
        "global-require": 0 ,
        "import/no-dynamic-require": 0,
        "max-len": 0
    },
    "parser": "babel-eslint"
}